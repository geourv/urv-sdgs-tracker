{
  "_id": "course_details_list",
  "startUrl": [
    "https://guiadocent.urv.cat/guido/public/centres/503/ensenyaments/3475/assignatures/gestionar"
  ],
  "selectors": [
    {
      "id": "table",
      "parentSelectors": ["_root"],
      "type": "SelectorElement",
      "multiple": true,
      "selector": "table[id^='listTable_']"
    },
    {
      "id": "course_row",
      "parentSelectors": ["table"],
      "type": "SelectorElement",
      "multiple": true,
      "selector": "tbody tr"
    },
    {
      "id": "code",
      "parentSelectors": ["course_row"],
      "type": "SelectorText",
      "selector": "td:nth-of-type(1)",
      "multiple": false
    },
    {
      "id": "url",
      "parentSelectors": ["course_row"],
      "type": "SelectorLink",
      "selector": "td:nth-of-type(2) a",
      "multiple": false
    },
    {
      "id": "period",
      "parentSelectors": ["course_row"],
      "type": "SelectorText",
      "selector": "td:nth-of-type(3)",
      "multiple": false
    },
    {
      "id": "type",
      "parentSelectors": ["course_row"],
      "type": "SelectorText",
      "selector": "td:nth-of-type(4)",
      "multiple": false
    },
    {
      "id": "credits",
      "parentSelectors": ["course_row"],
      "type": "SelectorText",
      "selector": "td:nth-of-type(5)",
      "multiple": false
    },
    {
      "id": "year",
      "parentSelectors": ["table"],
      "type": "SelectorText",
      "selector": "thead tr:nth-of-type(1)",
      "multiple": false
    }
  ]
}

